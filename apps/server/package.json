        "cors": "^2.8.5",
        "dotenv": "^17.2.1",
        "express": "^4.21.2",
        "ioredis": "^5.6.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^22.13.10",
        "esbuild": "^0.27.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2"
    },
    "engines": {
        "node": ">=22.0.0",
        "pnpm": ">=9.0.0"
    }
}
