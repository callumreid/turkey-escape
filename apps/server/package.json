{
    "name": "@turkey-escape/server",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "exports": {
        ".": "./src/shared/index.ts"
    },
    "scripts": {
        "build": "pnpm typecheck && pnpm clean && tsx esbuild.config.ts",
